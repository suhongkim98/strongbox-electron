{
  "name": "Accongbox",
  "version": "0.0.1",
  "author": "ssongkdev",
  "description": "Accong Box Desktop",
  "homepage": ".",
  "main": "build/electron/main.js",
  "private": true,
  "dependencies": {
    "@redux-devtools/extension": "^3.2.2",
    "@types/crypto-js": "^4.1.1",
    "@types/electron-devtools-installer": "^2.2.1",
    "@types/faker": "^6.6.8",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/sockjs-client": "^1.5.1",
    "@types/sqlite3": "^3.1.8",
    "@types/stompjs": "^2.3.5",
    "@types/styled-components": "^5.1.24",
    "aws-sdk": "^2.1087.0",
    "axios": "^0.26.0",
    "crypto-js": "^4.1.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^1.5.0",
    "faker": "^6.6.6",
    "lodash": "^4.17.21",
    "net": "^1.0.2",
    "react": "^16.13.1",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.8.3",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0",
    "react-toastify": "^8.2.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.2",
    "sockjs-client": "^1.6.0",
    "sqlite3": "^5.0.2",
    "stompjs": "^2.3.3",
    "styled-components": "^5.3.3",
    "styled-reset": "^4.3.4",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:windev": "concurrently \"SET BROWSER=none && yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "yarn build && tsc -p electron && electron-builder --win --x64",
    "eject": "react-scripts eject"
  },
  "build": {
    "productName": "Accong Box",
    "appId": "com.accongbox",
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "./public/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "language": 1042,
      "shortcutName": "accong box"
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13",
    "wait-on": "^6.0.1"
  }
}
